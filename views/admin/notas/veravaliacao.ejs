  <!-- BARRA DE PROGRESSO -->
<script>
  function atualizarPontuacao() {
    let pontuacaoAssiduidade = 0;
    let pontuacaoPontualidade = 0;
    let pontuacaoIniciativa = 0;
    let pontuacaoDisciplina = 0;
    let pontuacaoResponsabilidade = 0;
    let pontuacaoQualidade = 0;
    let pontuacaoAperfeicoamento = 0;

    // Calcular a pontuação para a assiduidade
    document.querySelectorAll('#div-assiduidade input[type="radio"]:checked').forEach((input) => {
        pontuacaoAssiduidade += parseInt(input.value);
    });

    // Calcular a pontuação para a pontualidade
    document.querySelectorAll('#div-pontualidade input[type="radio"]:checked').forEach((input) => {
      pontuacaoPontualidade += parseInt(input.value);
    });

    // Calcular a pontuação para a iniciativa
    document.querySelectorAll('#div-iniciativa input[type="radio"]:checked').forEach((input) => {
      pontuacaoIniciativa += parseInt(input.value);
    });

    // Calcular a pontuação para a disciplina
    document.querySelectorAll('#div-disciplina input[type="radio"]:checked').forEach((input) => {
      pontuacaoDisciplina += parseInt(input.value);
    });
    
    // Calcular a pontuação para a responsabilidade
    document.querySelectorAll('#div-responsabilidade input[type="radio"]:checked').forEach((input) => {
      pontuacaoResponsabilidade += parseInt(input.value);
    });

    // Calcular a pontuação para a qualidade
    document.querySelectorAll('#div-qualidade input[type="radio"]:checked').forEach((input) => {
      pontuacaoQualidade += parseInt(input.value);
    });

    // Calcular a pontuação para a aperfeicoamento
    document.querySelectorAll('#div-aperfeicoamento input[type="radio"]:checked').forEach((input) => {
      pontuacaoAperfeicoamento += parseInt(input.value);
    });

    // Atualizar todas as barras de progresso
    atualizarBarraProgresso('assiduidade', pontuacaoAssiduidade);
    atualizarBarraProgresso('pontualidade', pontuacaoPontualidade);
    atualizarBarraProgresso('iniciativa', pontuacaoIniciativa);
    atualizarBarraProgresso('disciplina', pontuacaoDisciplina);
    atualizarBarraProgresso('responsabilidade', pontuacaoResponsabilidade);
    atualizarBarraProgresso('qualidade', pontuacaoQualidade);
    atualizarBarraProgresso('aperfeicoamento', pontuacaoAperfeicoamento);

    // Atualizar todos os elementos de pontuação da assiduidade
    document.querySelectorAll('.totalScore-assiduidade').forEach((elemento) => {
        elemento.textContent = `${pontuacaoAssiduidade}`;
    });

    // Atualizar todos os elementos de pontuação da pontualidade
    document.querySelectorAll('.totalScore-pontualidade').forEach((elemento) => {
        elemento.textContent = `${pontuacaoPontualidade}`;
    });

    // Atualizar todos os elementos de pontuação da iniciativa
    document.querySelectorAll('.totalScore-iniciativa').forEach((elemento) => {
        elemento.textContent = `${pontuacaoIniciativa}`;
    });

    // Atualizar todos os elementos de pontuação da disciplina
    document.querySelectorAll('.totalScore-disciplina').forEach((elemento) => {
      elemento.textContent = `${pontuacaoDisciplina}`;
    });

    // Atualizar todos os elementos de pontuação da responsabilidade
    document.querySelectorAll('.totalScore-responsabilidade').forEach((elemento) => {
      elemento.textContent = `${pontuacaoResponsabilidade}`;
    });

     // Atualizar todos os elementos de pontuação da qualidade
     document.querySelectorAll('.totalScore-qualidade').forEach((elemento) => {
      elemento.textContent = `${pontuacaoQualidade}`;
    });

    // Atualizar todos os elementos de pontuação da aperfeicoamento
    document.querySelectorAll('.totalScore-aperfeicoamento').forEach((elemento) => {
      elemento.textContent = `${pontuacaoAperfeicoamento}`;
    });
    
    
  }

  function atualizarBarraProgresso(tipo, pontos) {
      const barras = document.querySelectorAll(`.progressBar-${tipo}`);
      const totalPerguntas = 5;
      const porcentagem = (pontos / (totalPerguntas * 2)) * 100;

      let cor = 'gray';
      if (pontos >= 1 && pontos <= 4) cor = 'red';
      else if (pontos >= 5 && pontos <= 7) cor = '#eeb818';
      else if (pontos >= 8 && pontos <= 10) cor = 'green';

      // Atualizar todas as barras do mesmo tipo
      barras.forEach((barra) => {
          barra.style.width = `${porcentagem}%`;
          barra.style.backgroundColor = cor;
      });
  }

  // Adicionar ouvintes de eventos para inputs
  document.querySelectorAll('input[type="radio"]').forEach((input) => {
      input.addEventListener('change', atualizarPontuacao);
  });

  // Chamada inicial para calcular a pontuação
  atualizarPontuacao();

</script>


